{
    "name": "11ty-wordpress-wpgraphql",
    "version": "0.1.0",
    "description": "Template for a project to grab data from a WordPress CMS using GraphQL and create an operational static 11ty website",
    "private": true,
    "scripts": {
        "build": "eleventy",
        "server": "eleventy --serve",
        "deploy": "rsync -avzh --delete --progress ~/Projects/project-name/public_html ssh-username@static.yourdomain.com:~/public_html"
    },
    "dependencies": {
        "@11ty/eleventy": "^0.12.1",
        "@11ty/eleventy-img": "^0.8.2",
        "dayjs": "^1.10.4",
        "eleventy-plugin-nesting-toc": "^1.3.0",
        "eleventy-plugin-reader-bar": "^0.2.0",
        "eleventy-plugin-reading-time": "0.0.1",
        "flat-cache": "^3.0.4",
        "jsdom": "^16.5.2",
        "node-fetch": "^2.6.1",
        "path": "^0.12.7",
        "shorthash2": "^1.0.3"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
